<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/seekbar_back" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/seekbar_second" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#99515c7a" android:endColor="#991094a9" android:angle="0.0" android:type="linear" android:centerX="0.45" android:centerY="0.45" android:centerColor="#99515c7a" />
            </shape>
        </clip>
    </item>
</layer-list>